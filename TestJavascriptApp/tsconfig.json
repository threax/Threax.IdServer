{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "obj/tscout/lib",
    "outFile": "wwwroot/lib/tsbin.js",
    "module": "amd",
    "baseUrl": ".",
    "noEmitHelpers": true,
    "moduleResolution": "node",
    "paths": {
      "clientlibs.*": [
        "Client\\Libs\\*"
      ],
      "hr.bootstrap.*": [
        "node_modules\\htmlrapier.bootstrap\\src\\*"
      ],
      "hr.halcyon-explorer.*": [
        "node_modules\\htmlrapier.halcyon-explorer\\src\\*"
      ],
      "hr.halcyon.*": [
        "node_modules\\htmlrapier.halcyon\\src\\*"
      ],
      "hr.widgets.*": [
        "node_modules\\htmlrapier.widgets\\src\\*"
      ],
      "hr.*": [
        "node_modules\\htmlrapier\\src\\*"
      ]
    }
  },
  "compileOnSave": true,
  "include": [
    "Client\\Libs\\**\\*.ts",
    "Views\\**\\*.ts",
    "node_modules\\htmlrapier.bootstrap\\src\\**\\*.ts",
    "node_modules\\htmlrapier.halcyon-explorer\\src\\**\\*.ts",
    "node_modules\\htmlrapier.halcyon\\src\\**\\*.ts",
    "node_modules\\htmlrapier.widgets\\src\\**\\*.ts",
    "node_modules\\htmlrapier\\src\\**\\*.ts"
  ],
  "exclude": [
    "node_modules\\threax-npm-tk\\**\\*.js"
  ],
  "files": [
    "node_modules\\jsns\\jsns.ts"
  ]
}