{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "https://localhost:44390/",
      "sslPort": 44390
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "https://localhost:44390/",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_preventHostingStartup": "True"
      }
    },
    "Threax.IdServer": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:59411/"
    },
    "SetupDb": {
      "commandName": "Project",
      "commandLineArgs": "tools migrate seed",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:64199/"
    },
    "UpdateClient": {
      "commandName": "Project",
      "commandLineArgs": "tools \"clientgen Client\\Libs\\IdServerClient.ts\" \"clientgen ..\\AppDashboard\\Client\\Libs\\IdServerClient.ts\" \"clientgen ..\\Threax.IdServer.Client\\ServiceClient.cs\"",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Update Config Schema": {
      "commandName": "Project",
      "commandLineArgs": "tools updateConfigSchema",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Purge Refresh Tokens": {
      "commandName": "Project",
      "commandLineArgs": "tools purgeRefreshTokens",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Create Test Rollover Cert": {
      "commandName": "Project",
      "commandLineArgs": "tools \"createCert rollover 25 ../Rollover.pfx\"",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}